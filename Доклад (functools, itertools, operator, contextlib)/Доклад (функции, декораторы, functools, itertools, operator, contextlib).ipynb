   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "{k: list(v) for k, v in itertools.groupby([('a', 'b'), ('a', 'c'), ('b', 'c')], lambda _: _[0])}"
   ]
